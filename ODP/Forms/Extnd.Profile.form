<item name='$WindowTitle' sign='true'><formula>"Extnd Profile Document"</formula></item>
<item name='$WEBQuerySave' sign='true'><formula>REM {trim, convert backslashes to forward slashes, and convert spaces to + character};
tmpUrlPath := @ReplaceSubstring(@Trim(ExtndDb);"\\":" ";"/":"+");

REM {make sure the first and last character is a forward slash};
tmpUrlPath2 := @If(@Right(tmpUrlPath;1) = "/"; tmpUrlPath; tmpUrlPath + "/");
tmpUrlPath3 := @If(@Left(tmpUrlPath2;1) = "/"; tmpUrlPath2;"/" + tmpUrlPath2);

REM {make sure the db path does not have any begining or ending slashes};
tmpDbPath := @If(@Right(tmpUrlPath;1) = "/"; @LeftBack(ExtndDb;1);ExtndDb);
tmpDbPath2 := @If(@Left(tmpDbPath;1) = "/"; @RightBack(tmpDbPath;1);tmpDbPath);

@SetProfileField("Extnd.Profile"; "ExtndDb"; tmpDbPath2);
@SetProfileField("Extnd.Profile"; "extUrl"; tmpUrlPath3 + "ext/");
@SetProfileField("Extnd.Profile"; "extndUrl"; tmpUrlPath3 + "extnd/")</formula></item>
